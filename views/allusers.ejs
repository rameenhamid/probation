<!DOCTYPE html>
<html lang="en">
<head>
    <!-- head content -->
</head>
<body>
    <div class="container">
        <h1>All Users</h1>
        <ul>
            <% users.forEach(user => { %>
                <li>
                    <a href="/users/<%= user.id %>">
                        <%= user.name %> | <%= user.email %> | <%= user.password %>
                    </a>
                </li>
            <% }); %>
        </ul>
    </div>
</body>
</html>
