<!DOCTYPE html>
<html lang="en">
<head>
    <!-- head content -->
</head>
<body>
    <div class="container">
        <h1>All Posts</h1>
        <ul>
            <% posts.forEach(post => { %>
                <li>
                    <a href="/users/<%= post.userId %>/posts/<%= post.id %>"><%= post.title %></a>
                </li>
            <% }); %>
        </ul>
    </div>
</body>
</html>
